{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[javascript,javascriptreact,typescript,typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.tabSize": 2,
  "files.eol": "\n",
  "cSpell.ignoreWords": [
    "Antioquia",
    "Dropright",
    "Envigado",
    "Medalla",
    "Nephrology",
    "Pe√±a",
    "Pneumology",
    "cbib",
    "echocardiography",
    "headlessui",
    "nsgqg",
    "signin",
    "splide",
    "splidejs",
    "uuidv"
  ],
  "cSpell.words": ["Firestore"]
}
